{
  "name": "moreamazingnick/yii2-fullcalendar",
  "description": "Yii2 widget for fullcalendar",
  "keywords": [
    "fullcalendar",
    "javascript",
    "event",
    "calendar"
  ],
  "homepage": "https://github.com/moreamazingnick/yii2-fullcalendar",
  "type": "yii2-extension",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Freriks",
      "email": "edofre@gmail.com"
    },
    {
      "name": "Nicolas Schneider",
      "email": "dev@nicolas-schneider.at"
    }
  ],
  "require": {
    "php": ">=5.5.0",
    "yiisoft/yii2": "*",
    "yiisoft/yii2-jui": "*"
  },
  "require-dev": {
    "codeception/codeception": "*"
  },
  "config": {
    "process-timeout": 1800,
    "fxp-asset": {
      "repositories": {
        "bower-asset/eve": {
          "type": "bower-vcs",
          "url": "https://github.com/adobe-webplatform/eve.git"
        }
      }
    }
  },
  "autoload": {
    "psr-4": {
      "moreamazingnick\\fullcalendar\\": "src"
    }
  }
}